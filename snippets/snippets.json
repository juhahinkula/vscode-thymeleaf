
{
  "th:text": {
    "prefix": "thtxt",
    "body": "th:text=\"${}\"",
    "description": "Thymeleaf text"
  },

  "th:field": {
    "prefix": "thfld",
    "body": "th:field=\"*{}\"",
    "description": "Thymeleaf field"
  },

  "th:each": {
    "prefix": "theach",
    "body": "th:each=\" : ${}\"",
    "description": "Thymeleaf each"
  },

  "th:each li": {
    "prefix": "theachli",
    "body": "<li th:each=\" : ${}\"></li>",
    "description": "Thymeleaf each list"
  }, 

