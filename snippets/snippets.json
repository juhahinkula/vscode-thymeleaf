    "prefix": "thobj",
    "body": "th:object=\"${}\"",
    "description": "Thymeleaf object"
  },  

  "th:ref": {
    "prefix": "thref",
    "body": "th:href=\"@{}\"" ,
    "description": "Thymeleaf reference"
  }, 
 
  "th:form": {
    "prefix": "thform",
    "body": "<form th:action=\"@{}\"></form>",
    "description": "Thymeleaf form"
  }, 

  "th:if": {
    "prefix": "thif",
    "body": "th:if=\"${}\"",
    "description": "Thymeleaf if"
  }, 

  "th:switch": {
    "prefix": "thswitch",
    "body": "th:switch=\"${}\"",
    "description": "Thymeleaf switch"
  },   

  "th:case": {
    "prefix": "thcase",
    "body": "th:case=\"\"",
    "description": "Thymeleaf case"
  },   

  "th:value": {
    "prefix": "thval",
    "body": "th:value=\"\"",
    "description": "Thymeleaf value"
  },   

  "th:replace": {
    "prefix": "threp",
    "body": "th:replace=\"\"",
    "description": "Thymeleaf replace"
  },   

  "th:insert": {
    "prefix": "thins",
    "body": "th:insert=\"\"",
    "description": "Thymeleaf insert"
